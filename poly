public class Poly {
    void add(int x,int y){
        System.out.println(x+y);
 }
 void add(int x,int y,int z){
     System.out.println(x+y+z);
 }
             public static void main(String[] args) {
                 Poly ad=new Poly();
                 ad.add(5,6);
                  ad.add(6,8,2);
 }
}
